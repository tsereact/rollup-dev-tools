{
    "name": "@tsereact/builder",
    "version": "1.1.1",
    "type": "module",
    "dependencies": {
        "@types/compression": "^1.7.2",
        "@types/node": "^18.7.13",
        "@types/picomatch": "^2.3.0",
        "@types/serve-static": "^1.15.0",
        "@types/ws": "^8.5.3",
        "@yarnpkg/cli": "3",
        "@yarnpkg/core": "3",
        "@yarnpkg/fslib": "^3.0.0-rc.24",
        "compression": "^1.7.4",
        "picomatch": "^2.3.1",
        "serve-static": "^1.15.0",
        "tslib": "^2.4.0",
        "ws": "^8.9.0"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^22.0.2",
        "@rollup/plugin-inject": "^4.0.4",
        "@rollup/plugin-node-resolve": "^14.1.0",
        "@rollup/plugin-typescript": "^8.4.0",
        "@swc/core": "^1.2.246",
        "@swc/jest": "^0.2.22",
        "@types/jest": "^29.0.0",
        "jest": "^29.0.2",
        "rollup": "^2.78.1",
        "typescript": "^4.8.4",
        "xterm": "^5.0.0",
        "xterm-addon-fit": "^0.6.0",
        "xterm-addon-web-links": "^0.7.0"
    },
    "exports": {
        "./*/index": {
            "require": "./dist/cjs/*.cjs",
            "import": "./dist/esm/*.mjs",
            "default": "./dist/esm/*.mjs"
        },
        "./*": {
            "require": "./dist/cjs/*.cjs",
            "import": "./dist/esm/*.mjs",
            "default": "./dist/esm/*.mjs"
        }
    },
    "typesVersions": {
        "*": {
            "src/*": [
                "src/*.ts",
                "src/*/index.ts"
            ],
            "*": [
                "dist/dts/*.d.ts",
                "dist/dts/*/index.d.ts",
                "src/*.ts",
                "src/*/index.ts"
            ]
        }
    },
    "scripts": {
        "build": "rollup -c rollup.config.mjs",
        "watch": "rollup -c rollup.config.mjs",
        "test": "jest"
    }
}
